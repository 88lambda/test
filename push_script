           Community/appliance_logo.svg \
           Community/logo_detail_page.svg \
           Resources/logo.svg \
           Community/onlyoffice-ds.ini \
           Community/preinst \
           Resources/setup \
           Resources/inst \
           Community/uinst \
           Community/test
         $appcenterctl upload $credentials --noninteractive $appver_ee \
           Enterprise/LICENSE_AGREEMENT_EN \
